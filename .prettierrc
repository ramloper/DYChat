{
  "printWidth": 140,
  "tabWidth": 4,
  "singleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "auto",
  "plugins": ["prettier-plugin-tailwindcss"],
  "tailwindCSS.classAttributes": ["class", "className", "ngClass"]
}
